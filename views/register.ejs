<%- include('./partials/header')%>

<form action="/register" method="post" class="w-[30%] p-5 bg-slate-400 m-auto">
    <input type="text" name="name" placeholder="name" class="w-full p-2 mt-5">
    <input type="text" name="username" placeholder="username" class="w-full p-2 mt-5">
    <input type="email" name="email" placeholder="email" class="w-full p-2 mt-5">
    <input type="password" name="password" placeholder="password" class="w-full p-2 mt-5">
    <button class="w-full p-2 mt-5 bg-black text-white">Register</button>
</form>

<%-include('./partials/footer')%>